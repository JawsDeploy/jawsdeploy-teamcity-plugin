<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
  <bean id="jawsDeployClient" class="com.jawsdeploy.JawsDeployClient"/>
  <bean id="jawsDeployController" class="com.jawsdeploy.JawsDeployController" autowire="constructor"/>
  <bean id="jawsDeployRunType" class="com.jawsdeploy.JawsDeployRunType" autowire="constructor"/>
  <bean id="jawsDeployBuildTab" class="com.jawsdeploy.JawsDeployBuildTab" autowire="constructor"/>
  <bean id="jawsDeployServerListener" class="com.jawsdeploy.JawsDeployServerListener" autowire="constructor"/>
</beans>